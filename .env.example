# ASOM Data Project — Environment Variables
# Copy this to .env and fill in your values.
# .env is gitignored — NEVER commit credentials.

# Environment: local | local-sqlite | dev | qa | prod
ASOM_ENV=local

# Snowflake credentials (only needed when ASOM_ENV=dev|qa|prod)
# ASOM_SNOWFLAKE_ACCOUNT=your_account.region
# ASOM_SNOWFLAKE_USER=your_username
# ASOM_SNOWFLAKE_DATABASE=DEV_DATABASE
# ASOM_SNOWFLAKE_WAREHOUSE=DEV_WH_XS
# ASOM_SNOWFLAKE_ROLE=DATA_ENGINEER

# Logging
ASOM_LOG_LEVEL=DEBUG
